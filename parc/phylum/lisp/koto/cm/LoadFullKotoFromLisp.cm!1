// LoadFullFromLisp.cm Edited 5-Sep-85 -- van Melle --Copy Init.save _ Init.LispDel INIT.LISP!*FTP/-E ERIS Login/C Dir/C Lisp>koto>basics Ret/<>A Lisp.run DoradoLispMc.eb DolphinLispMc.eb Ret/S Init.nogreet Init.LispLisp [ERIS]<Lisp>KOTO>BASICS>Lisp.sysout ;"  SHH(AND (INFILEP '{DSK}INIT.SAVE) (COPYFILE '{DSK}INIT.SAVE '{DSK}INIT.LISP;1) (DELFILE '{DSK}INIT.SAVE))SHH(LOAD '{ERINYES}<LISP>KOTO>CM>LOADFULL.LISP)SHH(LOAD '{ERINYES}<LISP>koto>patches>ALLOCPATCH.DCOM)SHH(LOAD '{ERINYES}<LISP>koto>patches>BITBLTPATCH.DCOM)SHH(LOAD '{ERINYES}<LISP>koto>patches>DISKPARTITIONPATCH.DCOM)SHH(LOAD '{ERINYES}<LISP>koto>patches>DOVEETHERPATCH.DCOM)SHH(LOAD '{ERINYES}<LISP>koto>patches>DOVEFLOPPYPATCH.DCOM)SHH(LOAD '{ERINYES}<LISP>koto>patches>DOVELFPATCH.DCOM)SHH(LOAD '{ERINYES}<LISP>koto>patches>FILENAMEFROMID.DCOM)SHH(LOAD '{ERINYES}<LISP>koto>patches>FLOPPYFORMATPATCH.DCOM)SHH(LOAD '{ERINYES}<LISP>koto>patches>FLOPPYPATCH.DCOM)SHH(LOAD '{ERINYES}<LISP>koto>patches>FLOPPYRENAMEFILEPATCH.DCOM)SHH(LOAD '{ERINYES}<LISP>koto>patches>HARDCOPYWPATCH.DCOM)SHH(LOAD '{ERINYES}<LISP>koto>patches>HPRINTPATCH.DCOM)SHH(LOAD '{ERINYES}<LISP>koto>patches>INSPECTPATCH.DCOM)SHH(LOAD '{ERINYES}<LISP>koto>patches>INTERPRESSPATCH.DCOM)SHH(LOAD '{ERINYES}<LISP>koto>patches>LFEQFIX.DCOM)SHH(LOAD '{ERINYES}<LISP>koto>patches>MIDDLEMOUSE.DCOM)SHH(LOAD '{ERINYES}<LISP>koto>patches>SA4000PATCH.DCOM)SHH(LOAD '{ERINYES}<LISP>koto>patches>vmemsizepatch.dcom)SHH(MAKESYS '{ERIS}<LISP>KOTO>BASICS>FULL.SYSOUT]SHH(LOGOUT T] "