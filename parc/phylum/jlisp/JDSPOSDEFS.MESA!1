-- JDSPosDefs.mesa-- last edit by: M.Ando 26-Jul-83 10:41:45DIRECTORY MSegment  USING [Handle];JDSPosDefs:DEFINITIONS=  BEGIN     DelPosSpace: PROCEDURE [pspace: MSegment.Handle] RETURNS [MSegment.Handle];      GetPosSpace: PROCEDURE [filename: LONG STRING] RETURNS [pspace: MSegment.Handle];      GetPosValue: PROCEDURE [pchr1, pchr2: CARDINAL] RETURNS [pos: [0..377B]];      GetPosChars: PROCEDURE [pos: [0..377B]] RETURNS [pchr1, pchr2: CARDINAL];      GetCatValue: PROCEDURE [catchar: CARDINAL ] RETURNS [cat: [0..377B]];      GetCatChar: PROCEDURE [cat: [0..377B]] RETURNS [catchar: CARDINAL];     END.      