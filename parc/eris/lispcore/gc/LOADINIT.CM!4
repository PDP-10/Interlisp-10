// LoadInit.cm edited: January 8, 1986  4:19 PM by MasinterDelete INIT.LISP!* INIT.SAVE!* // otherwise Lisp might read INIT.LISP!2Copy INIT.SAVE _ INIT.LISP  // save away site file to be restored belowFTP/-E ERIS Login/C Directory/C Lisp>Koto>Basics Ret/<>A Lisp.run DoradoLispMc.eb Ret/S <Lisp>Current>Init.NOGREET Init.LispLisp [ERIS]<Lisp>Koto>basics>LISP.Sysout ;"DELFILE({DSK}INIT.LISP)RENAMEFILE({DSK}INIT.SAVE {DSK}INIT.LISP)CONN {ERIS}<LispCore>SOURCES>(SETQ DIRECTORIES (APPEND '({ERIS}<LISPCORE>GC> {ERIS}<LISPCORE>SOURCES> {ERIS}<LISPCORE>LIBRARY>) DIRECTORIES))(* FILESETS has where to get things from)(LOAD 'FILESETS) (FILESLOAD RENAMEFNS MAKEINIT DLFIXINIT)(* Versions are Lisp Microcode Bcpl)(PROGN (CNDIR '{CORE}) (DORENAME 'I) (DLFIXINIT   (MAKEINIT '(113400Q 13040Q 24400Q)     NIL NIL     '({ERIS}<LISPCORE>GC> {ERIS}<LISPCORE>SOURCES>{ERIS}<LISPCORE>LIBRARY>{ERIS}<LISPCORE>LISPUSERS>{ERIS}<LISP>Koto>LIBRARY> {ERIS}<LISPUSERS>LUTE> {ERIS}<LISPUSERS>KOTO> {ERIS}<LISP>Koto>LISPUSERS> {ERIS}<LISPUSERS>))   '{ERIS}<LISPCORE>GC>INIT.DLINIT   '{ERIS}<LISPCORE>NEXT>DLISPDOMINO.DB) (LOGOUT T]] " // NO GC MC SHOULD BE INSTALLED FOR <LISPCORE>NEXT>NOGCMC.EB