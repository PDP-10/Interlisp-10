// LoadFullFromDLInitBVM.cm, edited June 13, 1986 bvm// bring over files necessary to run INIT.DLINIT.FTP/-E ERIS Directory/C LispCore>Next Ret/<>A Lisp.run DoradoLispMc.eb;// start up DLInit. MAKESYSNAME gets set here. The (SPECVARS . T) is because some file in the loadup (unknown) sets it wrongLisp [ERIS]<LispCore>bvm>Init.dlinit ;"(LOAD (QUOTE {ERIS}<LISPCORE>SOURCES>LOADUP.LISP))(SETQQ MAKESYSNAME LISPCORE)(HARDRESET] SHH(ENDLOADUP] SHH(SPECVARS . T]SHH(* MAKESYS '{ERIS}<LISPCORE>NEXT>LISP.SYSOUT]  SHH(LOAD '{ERIS}<LISPCORE>SOURCES>LOADFULL.LISP)SHH(MAKESYS '{ERIS}<LISPCORE>BVM>FULL.SYSOUT]SHH(LOGOUT T] "