       NIL                             (QUOTE RESETLST)))              (*                                             "If no version specified, enumerate only highest version")    (SETA SPELLFILE 1 (FUNCTION SPELLFILE1))    (FIXSPELL ROOTNAME NIL SPELLFILE (QUOTE NO-MESSAGE])(SPELLFILE1  [LAMBDA (ARR)                                              (* bvm: " 6-Jun-86 11:12")                    (* * "Name generator for a FIXSPELL -- generates files for a given host/directory, but returns names with the host/directory stripped off for fixspell matching.")    (DECLARE (USEDFREE VERSION))    (PROG (FL NAME1 EXT1 VERS#1)      LP  (COND             ([NULL (SETQ FL (\GENERATENEXTFILE (ELT ARR 2)                                    (NULL VERSION]              (RETURN)))          (for FIELDS on (UNPACKFILENAME.STRING FL) by (CDDR FIELDS)             do                                              (* Ignore host and directory, assuming                                                              we only generate appropriate ones.)                (SELECTQ (CAR FIELDS)                    (NAME (SETQ NAME1 (CADR FIELDS)))                    (EXTENSION (SETQ EXT1 (CADR FIELDS)))                    (VERSION (SETQ VERS#1 (CADR FIELDS)))                    NIL))          [COND             ([OR (NOT VERSION)                  (AND VERS#1 (EQ VERSION (MKATOM VERS#1]    (* Skip if versions mismatch, so                                                              fixspell only works on names)              (RETURN (PACKFILENAME.STRING (QUOTE NAME)                             NAME1                             (QUOTE EXTENSION)                             EXT1]          (SETQ NAME1 (SETQ EXT1 (SETQ VERS#1 NIL)))          (GO LP])(SPELLFILEDIR  [LAMBDA (DIR)                                              (* rmk: "13-NOV-81 22:13")                                                            