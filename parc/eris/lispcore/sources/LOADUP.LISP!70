(* "File <LispCore>Sources>Loadup.LISP Edited 29-Oct-86 23:35:49 by Masinter")(SETQQ COMPILE.EXT DCOM)(SETQ SYSFILES (UNION BOOTLOADEDFILES SYSFILES))(SETQ BOOTLOADEDFILES)(* (SETQ LITATOM-PACKAGE-CONVERSION-ENABLED T))(* "following files are really loaded earlier, this call to LOADUP just cleans up")(LOADUP (QUOTE (ACODE MACHINEINDEPENDENT POSTLOADUP)))(* "establish all package exports early")(LOADUP (QUOTE (XCL-PACKAGE LISP-PACKAGE FASL-PACKAGE COMPILER-PACKAGE)))(* "turn on FTP so that loadup will be faster")(LOADUP (QUOTE (BSP DPUPFTP)))(* "early runtime support for Common Lisp and (temporarily) debugger")(LOADUP (QUOTE (CMLCHARACTER CMLREADTABLE HELPDL STACKFNS BREAK CMLMACROS CMLMVS MACROS MACROAUX CMLSYMBOL CMLHASH CMLDEFFER CMLEVAL)))(LOADUP (QUOTE (COMMON CMLLIST CMLSPECIALFORMS ERROR-RUNTIME XCLC-RUNTIME CMLTYPES CL-ERROR)))(LOADUP (QUOTE (AFONT EDIT WEDIT PRETTY DSPRINTDEF NEWPRINTDEF FONTPROFILE SPELLFILE PRINTFN ADVISE LOADFNS DMISC DIRECTORY SPELLFILE FILEPKG RESOURCE)))(* "the bytecompiler and Interlisp compiler interface functions")(LOADUP (QUOTE (DLAP BYTECOMPILER COMPILE)))(LOADUP (QUOTE (HIST UNDO SPELL DWIM WTFIX CLISP DWIMIFY CLISPIFY RECORD ASSIST ASKUSER COMMON)))(DWIM (QUOTE C))(* "Kernel Common Lisp files")(LOADUP (QUOTE (CMLEXEC CMLDOC CMLPARSE  CMLSETF CMLPRED CMLTYPES CMLARRAY CMLARRAY-OPTIMIZERS CMLREAD CMLWALK CMLSEQ CMLARITH DEFSTRUCT CMLMISCIO CMLLIST CMLCOMPILE CMLSTRING CLSTREAMS CMLDESTRUCT CL-ERROR CMLFORMAT CMLENVIRONMENT CMLPRINT CMLLOAD CMLFILESYS CMLFLOAT CMLTIME CMLRAND CMLMODULES)))(LOADUP (QUOTE (CMLPATHNAME HPRINT ADDARITH MSANALYZE MSPARSE MASTERSCOPE AARITH ADISPLAY HLDISPLAY MENU WINDOWOBJ WINDOWSCROLL WINDOW WINDOWICON ATTACHEDWINDOW WBREAK XXGEOM XXFILL DEXEC INSPECTFRAME INSPECT EDITINTERFACE DEDIT TTYIN)))(* "Needs some fixup" (LOADUP (QUOTE (LOADUPPCL))))(LOADUP (QUOTE (DISKDLION DOVEINPUTOUTPUT DOVEDISK DOVEDISPLAY DOVEMISC DOVEETHER DOVEFLOPPY LOCALFILE DSKDISPLAY 10MBDRIVER LLNS TRSERVER BRKDWN MATCH SPP COURIER NSPRINT CLEARINGHOUSE NSFILING HARDCOPY PRESS PUPPRINT INTERPRESS FLOPPY IDLER ICONW FREEMENU SEDIT)))(LOADUP (QUOTE (D-ASSEM FASL XCL-COMPILER)))(* "CMLPACKAGE pushes onto INSPECTMACROS")(LOADUP (QUOTE (CMLPACKAGE)))(* "Already enabled, but this time fixes tables that weren't defined in the init")(PACKAGE-ENABLE)STOP