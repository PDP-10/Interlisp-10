{  Filename: dbDrawLineB0.mc   Description: Lisp line-drawing microcode.   Author: Mitch Lichtenberg   Creation date: 25-Jun-83 16:51:58   }@DRAWLINE:	opcode[73'b],	Bank _ DLBank,	c1;	uYCount _ TOS,	c2;	S _ S - 14'd, CROSS[DLEntry],	c3;	S _ S - 16'd,	c1, at[DLExit];	L2 _ L2.0, PC _ PC + PC16,  IBDisp,	c2;	L2 _ L2.0, DISPNI[OpTable],	c3;	{ E N D }