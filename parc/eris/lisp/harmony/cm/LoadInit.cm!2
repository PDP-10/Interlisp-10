// LoadInit.cm, edited 11 sep 84, MasinterDelete INIT.LISP!* // otherwise Lisp might read INIT.LISP!2Copy INIT.SAVE _ INIT.LISP  // save away site fileFTP ERIS Login/C Lisp.Auto WN11XS Directory/C Lisp>Harmony>Basics Ret/<>A Lisp.run DoradoLispMc.eb DolphinLispMc.eb Ret/S Init.nogreet Init.Lisp// run in LispCore>Good. If there weren't incompatible changes, Lisp>Current would doLisp [ERIS]<LispCore>Good>Lisp.Sysout ;" CONN {ERIS}<Lisp>HARMONY>SOURCES>(SETQ DIRECTORIES  '({ERIS}<LISP>HARMONY>SOURCES> {eris}<LISP>HARMONY>LIBRARY>))(* where to get things from)(LOAD 'FILESETS) (* FILESETS has names of where files)(FILESLOAD RENAMEFNS MAKEINIT DLFIXINIT)(PROGN (CNDIR '{CORE}) (DORENAME 'I) (DLFIXINIT   (MAKEINIT '(111000Q 12000Q 21400Q)      NIL NIL      '({ERIS}<LISP>HARMONY>SOURCES> {eris}<LISP>HARMONY>LIBRARY>        {ERIS}<LISP>HARMONY>LISPUSERS> {eris}<LISPUSERS>))   '{ERIS}<LISP>HARMONY>BASICS>INIT.DLINIT   '{ERIS}<LISP>HARMONY>BASICS>DLISPDOMINO.DB) (LOGOUT T]] " Copy INIT.LISP _ INIT.SAVEDelete INIT.SAVE