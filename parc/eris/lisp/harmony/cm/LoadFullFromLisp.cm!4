// LoadFullFromLisp.cmDelete INIT.LISP!* // otherwise Lisp might read INIT.LISP!2Copy INIT.SAVE _ INIT.LISP  // save away site fileFTP ERIS Login/C Lisp.Auto WN11XS Directory/C Lisp>Harmony>Basics Ret/<>A Lisp.run DoradoLispMc.eb DolphinLispMc.eb Ret/S Init.nogreet Init.LispLisp [ERIS]<Lisp>Harmony>Basics>Lisp.sysout ;"   SHH(SETQQ DISPLAYFONTDIRECTORIES ({ERIS}<LISP>FONTS> {INDIGO}<ALTOFONTS> {INDIGO}<ALTOFONTS>ORIGINAL>))SHH(SETQQ DISPLAYFONTEXTENSIONS (DISPLAYFONT STRIKE AC))SHH(SETQQ PRESSFONTWIDTHSFILES ({ERIS}<LISP>FONTS>FONTS.WIDTHS {INDIGO}<FONTS>FONTS.WIDTHS))SHH(SETQQ INTERPRESSFONTDIRECTORIES ({ERIS}<LISP>FONTS> {INDIGO}<ALTOFONTS> {INDIGO}<ALTOFONTS>ORIGINAL>))LOADUP((DPUPFTP TEDIT READNUMBER EDITBITMAP MAILCLIENT GRAPEVINE LAFITE MTP FILEBROWSER READSYS RDSYS REMOTEVMEM GRAPHER SPY SINGLEFILEINDEX AREDIT WHEREIS]ENDLOADUP] "Copy INIT.LISP _ INIT.SAVEDelete INIT.SAVE